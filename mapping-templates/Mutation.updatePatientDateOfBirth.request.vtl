{
    "version" : "2018-05-29",
    "operation" : "UpdateItem",
    "key": {
        "patientId": $util.dynamodb.toDynamoDBJson($context.arguments.patientId)
    },
    "update" : {
        "expression" : "set #dateOfBirth = :dateOfBirth",
        "expressionNames" : {
           "#dateOfBirth" : "dateOfBirth"
       },
       "expressionValues" : {
            ":dateOfBirth" : $util.dynamodb.toDynamoDBJson($context.arguments.dateOfBirth)
       }
    }
}