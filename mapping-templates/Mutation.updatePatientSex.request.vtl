{
    "version" : "2018-05-29",
    "operation" : "UpdateItem",
    "key": {
        "patientId": $util.dynamodb.toDynamoDBJson($context.arguments.patientId)
    },
    "update" : {
        "expression" : "set #sex = :sex",
        "expressionNames" : {
           "#sex" : "sex"
       },
       "expressionValues" : {
            ":sex" : $util.dynamodb.toDynamoDBJson($context.arguments.sex)
       }
    }
}