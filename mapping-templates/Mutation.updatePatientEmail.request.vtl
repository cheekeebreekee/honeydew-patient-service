{
    "version" : "2018-05-29",
    "operation" : "UpdateItem",
    "key": {
        "patientId": $util.dynamodb.toDynamoDBJson($context.arguments.patientId)
    },
    "update" : {
        "expression" : "set #email = :email",
        "expressionNames" : {
           "#email" : "email"
       },
       "expressionValues" : {
            ":email" : $util.dynamodb.toDynamoDBJson($context.arguments.email)
       }
    }
}